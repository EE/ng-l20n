<plural($n) {
    $n == 0 ? "zero" :
    $n == 1 ? "one" :
              "many"
}>

<objectsWithCount[plural($objectsNum)] {
    zero: "no objects",
    one: "1 object",
    many: "{{ $objectsNum }} objects",
}>

<setObjectsNumber "Set objects number">

<overlayed "This paragraph tests <strong>overlaying nodes; you should see the entered number in the bracket</strong>. Test number: <input>.">
